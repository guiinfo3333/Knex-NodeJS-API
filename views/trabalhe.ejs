.rodape{
    background-color: #141418;
    color: #fff;
    display: flex;
    justify-content: space-around;
    position: relative; 
    top: 100px;
}

.sec_rod{
    border:1px solid #141418;
    margin: 10px;
}

.img_rod{
    width: inherit;
}

#rodapelogo{
    width: 100px;
    height: 100px;
    top: 20px;
    position: relative;
}

.rodapemesage{
    position: relative;
    margin-left: -12px;
}

.rodapemesage:link {
    color: white;
    text-decoration: none;
}

.rodapemesage:visited{
    color: white;
}

.rodapemesage:hover{
    text-decoration: underline;
    color: gray;
}

section{
    position: relative;
    margin-left: 300px;
}

.checkbtn {
    font-size: 30px;
    color: white;
    float: right;
    line-height: 80px;
    margin-right: 40px;
    cursor: pointer;
    display: none;
}
#check {
    display: none;
}
@media (max-width: 952px){
    nav ul li a{
        font-size: 16px;
    }
}
@media (max-width: 858px){
    .checkbtn {
        display: block;
    }
    ul{
        position: fixed;
        width: 100%;
        height: 100vh;
        background: #1d075f;
        top: 80px;
        left: -100%;
        text-align: center;
        transition: all .5s;
    }
    nav ul li{
        display: block;
        margin: 50px 0;
        line-height: 30px;
    }
    nav ul li a {
        font-size: 20px;
    }
    a:hover, a.active {
        background: none;
        color: #5434af;
    }
    #check:checked ~ ul {
        left: 0;
    }

section{
    position: relative;
    margin-left: 0px;
}


}
</style>
