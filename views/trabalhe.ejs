    width: 100px;
}

.logo {
    position:relative;
    top: 0px;
    left: 0px;
    height: 100px;
    width: 100px;
}

header ul {
    float: right;
    position: relative;
    margin-top: 0px;
    line-height: 80px;
}

header ul a{
    text-decoration: none;
    color: white;
    line-height: normal;
    -webkit-transition: all 500ms ease;
    -o-transition: all 500ms ease;
    transition: all 500ms ease;
}

header ul li {
    display: inline-block;
    font-size: 15px;
    font-weight: bold; 
}

header ul li a:hover {
    background: black;
    border-radius: 25px;
}

a {
    padding: 10px
}

img {
    padding-top: 15px;
    padding-right: 5px;
}

b1 {
